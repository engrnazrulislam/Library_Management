{
    "builds":[
        {
            "src": "library_management/wsgi.py",
            "use": "@vercel/python",
            "config": {"maxLambdaSize": "20mb", "runtime": "python3.11.3"}
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "library_management/wsgi.py"
        }
    ]
}